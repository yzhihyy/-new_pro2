{% extends 'base.twig' %}

{% block title %}{% endblock %}

{% block metas %}
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="format-detection" content="telephone=no">
    <meta http-equiv="X-UA-Compatible" content="edge">
{% endblock %}

{% block stylesheets %}
    {% block common_css %}
        <link rel="stylesheet" type="text/css" href="{{ asset('/h5/css/base.css') }}"/>
    {% endblock %}

    {% block piecemeal_css %}
    {% endblock %}
{% endblock %}


{% block javascripts %}
    {% block common_js %}
        <script type="text/javascript" src="{{ asset('/h5/js/ydui.flexible.js') }}"></script>
        <script type="text/javascript" src="{{ asset('/h5/js/jquery-3.3.1.min.js') }}"></script>
    {% endblock %}

    {% block piecemeal_js %}
    {% endblock %}
{% endblock %}

{% block body %}
    {% block content %}
    {% endblock %}
{% endblock %}